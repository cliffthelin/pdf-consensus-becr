{
  "field_attributes_schema": {
    "description": "Schema for field_attributes in function catalog entries",
    "type": "object",
    "patternProperties": {
      "^.*$": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "description": "Expected data type (e.g., 'float', 'str', 'List[SeedBlock]')"
          },
          "constraint": {
            "type": "string", 
            "description": "Validation constraints (e.g., '>= 0.0', 'not empty')"
          },
          "description": {
            "type": "string",
            "description": "Field purpose and usage"
          },
          "default": {
            "description": "Default value if applicable"
          },
          "calculation": {
            "type": "string",
            "description": "How value is calculated if applicable"
          },
          "required_keys": {
            "type": "array",
            "items": {"type": "string"},
            "description": "Required keys for dictionary returns"
          }
        },
        "required": ["type", "description"]
      }
    }
  },
  "enhanced_attributes": {
    "field_attributes": {
      "type": "object",
      "description": "Detailed specifications for each field the function interacts with"
    },
    "side_effects": {
      "type": "array",
      "items": {"type": "string"},
      "description": "List of side effects the function may have"
    },
    "dependencies": {
      "type": "array", 
      "items": {"type": "string"},
      "description": "List of external dependencies required"
    }
  },
  "examples": {
    "basic_validation": {
      "field_attributes": {
        "self.x": {
          "type": "float",
          "constraint": ">= 0.0", 
          "description": "X coordinate of top-left corner"
        }
      },
      "side_effects": ["Raises ValueError if coordinates invalid"],
      "dependencies": ["dataclasses"]
    },
    "complex_return": {
      "field_attributes": {
        "return_value": {
          "type": "Dict[str, Any]",
          "constraint": "complete processing results",
          "description": "Processing results with pages and summary",
          "required_keys": ["pdf_path", "pages", "summary"]
        }
      },
      "side_effects": ["Processes PDF pages", "Saves results to file"],
      "dependencies": ["Path", "file_manager"]
    }
  }
}